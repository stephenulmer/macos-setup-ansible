<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<array>
{% for tr in keyboard_replacements %}
	<dict>
		<key>phrase</key>
		<string>{{ tr.with }}</string>
		<key>shortcut</key>
		<string>{{ tr.replace }}</string>
	</dict>
{% endfor %}
</array>
</plist>
